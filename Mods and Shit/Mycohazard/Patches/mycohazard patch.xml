<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!-- Mycohazard - A biological Mechanitor Mod -->
	<!-- Package ID: thatthing.mycohazard -->
	<!-- Author: 3564775477 -->

	<!-- Move DE_Fungal category into Automation as a nested subcategory -->
	<Operation Class="PatchOperationAddModExtension">
		<success>Always</success>
		<xpath>Defs/DesignationCategoryDef[defName="DE_Fungal"]</xpath>
		<value>
			<li Class="BetterArchitect.NestedCategoryExtension">
				<parentCategory>Ferny_Automation</parentCategory>
			</li>
		</value>
	</Operation>

</Patch>
